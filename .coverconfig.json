
{
    "enabled": true,
    "relativeSourcePath": "..",
    "relativeCoverageDir": "../../coverage",
    "ignorePatterns": ["**/node_modules/**", "test/**"],
    "includePid": false,
    "reports": ["json", "html", "lcov", "cobertura"],
    "verbose": true,
    "extends": "@istanbuljs/nyc-config-typescript",
    "branches": 80,
    "lines": 80,
    "functions": 80,
    "statements": 80,
    "reportDir": "../../coverage",
    "tempDir": "../../.nyc_output",
    "excludeNodeModules": true,
    "all": false,
    "sourceMap": false,
    "instrument": false,
    "include": [
        "../../src/**/*.ts"
    ],
    "exclude": [
        "node_modules",
        ".vscode-test",
        "coverage",
        "test-files",
        "../../src/**/*.test.ts",
        "src/**/*.d.ts"
    ],
    "reporter": [
        "text",
        "json",
        "html",
        "lcov",
        "cobertura"
    ]
}